meta {
  name: Delete Member
  type: http
  seq: 5
}

delete {
  url: {{base_url}}/orgs/{{org_id}}/members/{{member_id}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{org_member_api_key}}
}

docs {
  Soft-delete an org member (requires owner role).

  This performs a soft delete - the member can be restored later using
  POST /orgs/{org_id}/members/{member_id}/restore.

  Cannot delete yourself.

  Returns:
  { "success": true }
}
