meta {
  name: List Member API Keys (Impersonated)
  type: http
  seq: 10
}

get {
  url: {{base_url}}/orgs/{{org_id}}/members/{{user_id}}/api-keys
  body: none
  auth: bearer
}

headers {
  X-On-Behalf-Of: {{user_id}}
}

auth:bearer {
  token: {{operator_api_key}}
}

docs {
  List API keys for an org member using operator impersonation.

  Requirements:
  - Operator must have admin or owner role
  - X-On-Behalf-Of must be a user_id of a member in this org
}
